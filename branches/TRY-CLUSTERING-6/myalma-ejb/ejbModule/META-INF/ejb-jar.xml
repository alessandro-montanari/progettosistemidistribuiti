<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:ejb="http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
	version="3.1">
	<display-name>myalma-ejb</display-name>

	<enterprise-beans>
		<session>
			<ejb-name>EditContentBean</ejb-name>
			<ejb-class>it.unibo.myalma.business.professor.EditContentBean</ejb-class>
		</session>
		
		<session>
			<ejb-name>EditMaterialBean</ejb-name>
			<ejb-class>it.unibo.myalma.business.professor.EditMaterialBean</ejb-class>
		</session>
	</enterprise-beans>
	
	<interceptors>
		<interceptor>
			<interceptor-class>org.jboss.seam.ejb.SeamInterceptor</interceptor-class>
		</interceptor>
	</interceptors>
	
	<assembly-descriptor>
		<interceptor-binding>
			<ejb-name>*</ejb-name>
			<interceptor-class>org.jboss.seam.ejb.SeamInterceptor</interceptor-class>
		</interceptor-binding>
	</assembly-descriptor>
	
	
</ejb-jar>